{
  "scripts": {
    "serve": "parcel watch app/client/index.pug -d app/client/dist & nodemon app/server/app.js",
    "build": "rm -rf app/client/dist && parcel build app/client/index.pug -d app/client/dist",
    "build:run": "yarn build && node app/server/app.js",
    
    "docker:build": "docker build -t fermin-notes .",
    "docker:run": "docker run -p 8002:8002 fermin-notes",
    "docker:kill": "docker kill $(docker ps -q)",
    "docker:prune": "docker system prune -a"
  },
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "immutability-helper": "^2.9.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-quill": "^1.3.3"
  },
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-sw-precache": "^1.0.3",
    "nodemon": "^1.18.9",
    "pug": "^2.0.3",
    "sass": "^1.15.2"
  }
}
