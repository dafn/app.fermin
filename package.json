    "setcred": "export GOOGLE_APPLICATION_CREDENTIALS=keys/datastore-service-account-key.json"
  },
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "immutability-helper": "^2.9.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-quill": "^1.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "sass": "^1.15.2"
  }
}
