{
  "scripts": {
    "start": "NODE_ENV=production node server/app.js",
    "dev": "rm -rf dist && parcel watch client/index.pug & NODE_ENV=development nodemon server/app.js",
    "build": "rm -rf dist && parcel build client/index.pug --no-source-maps",
    "build:run": "yarn build &&  NODE_ENV=production node server/app.js",
    "docker:build": "docker build -t fermin-notes .",
    "docker:run": "docker run -p 8002:8002 fermin-notes",
    "docker:kill": "docker kill $(docker ps -q)",
    "docker:prune": "docker system prune -a",
    "deploy": "yarn build && gcloud app deploy"
  },
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "assets": "^3.0.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "graphql-request": "^1.8.2",
    "immutability-helper": "^2.9.0",
    "memorystore": "^1.6.0",
    "openid-client": "^2.4.5",
    "passport": "^0.4.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-quill": "^1.3.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "nodemon": "^1.18.9",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-sw-precache": "^1.0.3",
    "pug": "^2.0.3",
    "sass": "^1.15.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  }
}
