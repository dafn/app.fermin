{
  "scripts": {
    "start": "NODE_ENV=production node packages/f-server/app.js",
    "dev": "rm -rf dist && PARCEL_AUTOINSTALL=false parcel watch packages/f-app/index.pug & NODE_ENV=development nodemon packages/f-server/app.js",
    "build": "rm -rf dist && parcel build packages/f-app/index.pug --no-source-maps",
    "build:run": "NODE_ENV=production && yarn build && node packages/f-server/app.js",
    "clean": "rm -rf .cache rm -rf dist",
    "docker:build": "docker build -t app.fermin .",
    "docker:run": "docker run -p 8002:8002 app.fermin",
    "docker:kill": "docker kill $(docker ps -q)",
    "docker:prune": "docker system prune -a",
    "deploy": "yarn build && gcloud app deploy",
    "prettier:write": "prettier --write packages"
  },
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "assets": "^3.0.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "graphql-request": "^1.8.2",
    "immutability-helper": "^2.9.0",
    "memorystore": "^1.6.0",
    "openid-client": "^2.4.5",
    "passport": "^0.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-quill": "^1.3.3",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-syntax-json-strings": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "nodemon": "^1.18.9",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-sw-precache": "^1.1.1",
    "prettier": "^2.0.2",
    "pug": "^2.0.3",
    "sass": "^1.15.2",
    "typescript": "^3.5.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-syntax-json-strings"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "alias": {
    "f-app-core": "./packages/f-app-core",
    "f-app-notes": "./packages/f-app-notes",
    "f-app-hub": "./packages/f-app-hub"
  },
  "name": "app.fermin"
}
